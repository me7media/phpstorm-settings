<keymap version="1" name="Default copy" parent="Default for XWin">
  <action id="$Undo">
    <keyboard-shortcut first-keystroke="ctrl z" />
    <keyboard-shortcut first-keystroke="alt back_space" />
    <keyboard-shortcut first-keystroke="shift z" />
  </action>
  <action id="Editor Undo">
    <keyboard-shortcut first-keystroke="shift z" />
  </action>
  <action id="ResetToMySettings">
    <keyboard-shortcut first-keystroke="shift ctrl alt close_bracket" />
  </action>
  <action id="WD.UploadCurrentRemoteFileAction">
    <keyboard-shortcut first-keystroke="ctrl s" />
    <keyboard-shortcut first-keystroke="shift alt q" />
  </action>
</keymap>